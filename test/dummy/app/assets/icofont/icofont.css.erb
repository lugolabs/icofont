@font-face {
  font-family: "icofont";
  src: url(<%= asset_path "icofont/icofont.eot" %>);
  src: url(<%= asset_path "icofont/icofont.eot?#iefix" %>) format("embedded-opentype"),
       url(<%= asset_path "icofont/icofont.woff" %>) format("woff"),
       url(<%= asset_path "icofont/icofont.ttf" %>) format("truetype"),
       url(<%= asset_path "icofont/icofont.svg#icofont" %>) format("svg");
  font-weight: normal;
  font-style: normal;
}

[data-icon]:before { content: attr(data-icon); }

[data-icon]:before,
.icofont-aid:before,
.icofont-airplane:before,
.icofont-alarm2:before,
.icofont-android:before,
.icofont-angry:before,
.icofont-angry2:before,
.icofont-apple:before,
.icofont-arrow-down-left:before,
.icofont-arrow-down-left2:before,
.icofont-arrow-down-right2:before,
.icofont-arrow-down-right3:before,
.icofont-arrow-down:before,
.icofont-arrow-down4:before,
.icofont-arrow-left3:before,
.icofont-arrow-right:before,
.icofont-arrow-up-left3:before,
.icofont-brightness-contrast:before,
.icofont-brightness-medium:before {
  display: inline-block;
  font-family: "icofont";
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  line-height: 1;
  text-decoration: inherit;
  text-transform: none;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
}

.icofont-aid:before { content: "\f100"; }
.icofont-airplane:before { content: "\f101"; }
.icofont-alarm2:before { content: "\f102"; }
.icofont-android:before { content: "\f103"; }
.icofont-angry:before { content: "\f104"; }
.icofont-angry2:before { content: "\f105"; }
.icofont-apple:before { content: "\f106"; }
.icofont-arrow-down-left:before { content: "\f107"; }
.icofont-arrow-down-left2:before { content: "\f108"; }
.icofont-arrow-down-right2:before { content: "\f109"; }
.icofont-arrow-down-right3:before { content: "\f10a"; }
.icofont-arrow-down:before { content: "\f10b"; }
.icofont-arrow-down4:before { content: "\f10c"; }
.icofont-arrow-left3:before { content: "\f10d"; }
.icofont-arrow-right:before { content: "\f10e"; }
.icofont-arrow-up-left3:before { content: "\f10f"; }
.icofont-brightness-contrast:before { content: "\f110"; }
.icofont-brightness-medium:before { content: "\f111"; }
